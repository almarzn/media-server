{
  "samba-container-config": "v0",
  "configs": {
    "almachine": {
      "shares": [
        "share"
      ],
      "globals": [
        "default"
      ],
      "instance_name": "SAMBA"
    }
  },
  "shares": {
    "share": {
      "options": {
        "path": "/share",
        "valid users": "alma"
      }
    }
  },
  "globals": {
    "default": {
      "options": {
        "security": "user",
        "server min protocol": "SMB2",
        "load printers": "no",
        "printing": "bsd",
        "printcap name": "/dev/null",
        "disable spoolss": "yes",
        "guest ok": "no"
      }
    }
  },
  "users": {
    "all_entries": [
      {
        "name": "alma",
        "password": "{{ private.samba_password }}"
      }
    ]
  },
  "_footer": 1
}